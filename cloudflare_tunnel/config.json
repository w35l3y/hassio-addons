{
  "name": "Cloudflare Tunnel",
  "version": "1.2.5",
  "slug": "cloudflare-tunnel",
  "description": "Cloudflare Tunnel for Home Assistant",
  "url": "https://github.com/w35l3y/hassio-addons/tree/main/cloudflare_tunnel",
  "ingress": false,
  "panel_icon": "mdi:docker",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "map": ["config:rw"],
  "startup": "services",
  "boot": "auto",
  "ports": {
    "41705/tcp": 41705
  },
  "options": {
    "no-autoupdate": true,
    "originRequest": {},
    "metrics": "localhost:41705",
    "ingress": [
      {
        "hostname": "",
        "service": "http://ha_local_ip:8123"
      },
      {
        "service": "http_status:404"
      }
    ],
    "tunnel": "homeassistant"
  },
  "schema": {
    "tunnel": "str?",
    "credentials-file": "str?",
    "originRequest": {
      "connectTimeout": "str?",
      "tlsTimeout": "str?",
      "tcpKeepAlive": "str?",
      "noHappyEyeballs": "bool?",
      "keepAliveConnections": "int(0,)?",
      "keepAliveTimeout": "str?",
      "httpHostHeader": "str?",
      "originServerName": "str?",
      "caPool": "str?",
      "noTLSVerify": "bool?",
      "disableChunkedEncoding": "bool?",
      "proxyAddress": "str?",
      "proxyPort": "port?",
      "proxyType": "str?"
    },
    "no-autoupdate": "bool?",
    "loglevel": "str?",
    "logfile": "str?",
    "metrics": "str?",
    "url": "str?",
    "ingress": [
      {
        "hostname": "str?",
        "path": "str?",
        "service": "str?"
      }
    ]
  }
}
