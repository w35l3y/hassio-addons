{
  "name": "Cloudflare Tunnel",
  "version": "1.1.2",
  "slug": "cloudflare-tunnel",
  "description": "Cloudflare Tunnel for Home Assistant",
  "url": "https://github.com/w35l3y/hassio-addons/cloudflare-tunnel",
  "ingress": false,
  "panel_icon": "mdi:docker",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "map": ["config:rw"],
  "startup": "services",
  "boot": "auto",
  "options": {
    "no-autoupdate": true,
    "ingress": [
      {
        "hostname": "",
        "service": "http://homeassistant.local:8123"
      },
      {
        "service": "http_status:404"
      }
    ]
  },
  "schema": {
    "tunnel": "str?",
    "credentials-file": "str?",
    "no-autoupdate": "bool?",
    "loglevel": "str?",
    "logfile": "str?",
    "metrics": "str?",
    "url": "str?",
    "ingress": [
      {
        "service": "str?",
        "hostname": "str?",
        "path": "str?"
      }
    ]
  }
}
